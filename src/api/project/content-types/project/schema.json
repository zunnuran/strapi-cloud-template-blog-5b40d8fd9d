{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "short_description": {
      "type": "string"
    },
    "long_description": {
      "type": "richtext"
    },
    "hero_picture_video": {
      "type": "string"
    },
    "development_duration": {
      "type": "integer"
    },
    "service_type": {
      "type": "string"
    },
    "hero_picture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "galley": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "technologies": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "AR",
        "VR",
        "UE"
      ]
    },
    "genre": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Adventure",
        "Platformer",
        "Shooter",
        "Fighting",
        "Beat 'em up",
        "Stealth",
        "Survival",
        "Survival Horror",
        "Rhythm"
      ]
    },
    "url_play_store": {
      "type": "string"
    },
    "url_app_store": {
      "type": "string"
    },
    "slider_gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "page_gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "app_icon": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ratings": {
      "type": "integer",
      "min": 1,
      "max": 5
    },
    "shallow_background": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
